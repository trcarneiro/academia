{
    "name": "krav-maga-academy-backend",
    "version": "1.0.0",
    "description": "Sistema de gestÃ£o para academia de Krav Maga com IA integrada",
    "main": "dist/server.js",
    "scripts": {
        "dev": "set \"NODE_OPTIONS=-r tsconfig-paths/register\" && tsx watch src/server.ts",
        "dev:simple": "node servers/server-simple.js",
        "vercel-build": "prisma generate && (prisma migrate deploy || prisma db push --skip-generate) && tsc || echo 'Build completed with warnings'",
        "build": "tsc",
        "start": "node dist/server.js",
        "start:simple": "node servers/server-simple.js",
        "ci": "npm run type-check && npm run lint && npm run test -- --run && npm run guard:no-modals",
        "guard:no-modals": "node scripts/ci/no-modals-guard.js",
        "kiosk": "node servers/kiosk-server.js",
        "kiosk:dev": "nodemon servers/kiosk-server.js",
        "clean:simulation": "node scripts/clean-simulation-data.js",
        "clean:simulation:confirm": "node scripts/clean-simulation-data.js --confirm",
        "asaas:import": "node scripts/asaas-import.js",
        "asaas:import:dry": "node scripts/asaas-import.js --dry-run",
        "asaas:import:limit": "node scripts/asaas-import.js --limit=10",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:seed": "tsx prisma/seed.ts",
        "seed:demo": "tsx scripts/seed-demo-data.ts",
        "seed:quick": "tsx scripts/seed-quick-demo.ts",
        "seed:graduation": "tsx scripts/seed-graduation-test.ts",
        "clean:demo": "tsx scripts/clean-demo-safe.ts",
        "reset:demo": "tsx scripts/reset-demo.ts",
        "reset:demo:quick": "tsx scripts/reset-demo.ts --quick",
        "sync:google-ads": "tsx scripts/sync-google-ads.ts",
        "import:google-ads": "tsx scripts/import-real-google-ads-data.ts",
        "db:studio": "prisma studio",
        "db:reset": "npx prisma migrate reset --force",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:ui": "vitest --ui",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "type-check": "tsc --noEmit",
        "cert:generate": "powershell -ExecutionPolicy Bypass -File ./scripts/generate-cert.ps1",
        "cert:trust": "powershell -ExecutionPolicy Bypass -File ./scripts/trust-cert.ps1",
        "dev:https": "set \"USE_HTTPS=true\" && set \"NODE_OPTIONS=-r tsconfig-paths/register\" && tsx watch src/server.ts"
    },
    "keywords": [
        "krav-maga",
        "academy",
        "management",
        "ai",
        "fastify",
        "prisma"
    ],
    "author": "Academia System",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.24.3",
        "@fastify/cors": "^10.0.0",
        "@fastify/helmet": "^13.0.0",
        "@fastify/jwt": "^10.0.0",
        "@fastify/multipart": "^9.2.1",
        "@fastify/rate-limit": "^10.0.0",
        "@fastify/static": "^8.2.0",
        "@google/generative-ai": "^0.24.1",
        "@modelcontextprotocol/sdk": "^1.20.2",
        "@prisma/client": "^5.22.0",
        "@sendgrid/mail": "^8.1.6",
        "@supabase/supabase-js": "^2.38.4",
        "@types/joi": "^17.2.2",
        "@types/multiparty": "^4.2.1",
        "@types/ws": "^8.18.1",
        "aws-sdk": "^2.1501.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^2.4.3",
        "bullmq": "^5.1.9",
        "cors": "^2.8.5",
        "csv-parse": "^6.1.0",
        "csv-parser": "^3.2.0",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.10",
        "dotenv": "^16.6.0",
        "express": "^4.18.2",
        "face-api.js": "^0.20.0",
        "fastify": "^5.0.0",
        "google-ads-api": "^21.0.1",
        "googleapis": "^161.0.0",
        "joi": "^18.0.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^4.2.1",
        "node-fetch": "^2.7.0",
        "openai": "^4.24.1",
        "pg": "^8.16.3",
        "pino": "^8.17.2",
        "pino-pretty": "^10.3.1",
        "posthog-node": "^3.6.0",
        "qrcode": "^1.5.3",
        "redis": "^4.6.12",
        "sharp": "^0.32.6",
        "slugify": "^1.6.6",
        "stripe": "^14.9.0",
        "twilio": "^4.23.0",
        "uuid": "^9.0.1",
        "ws": "^8.18.3",
        "zod": "^3.22.4",
        "zod-to-json-schema": "^3.22.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.6",
        "@types/node-cron": "^3.0.11",
        "@types/qrcode": "^1.5.5",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "@vitest/ui": "^3.2.4",
        "axios": "^1.11.0",
        "chalk": "^5.4.1",
        "eslint": "^8.56.0",
        "jest": "^30.0.5",
        "prettier": "^3.1.1",
        "prisma": "^5.7.1",
        "supertest": "^7.1.4",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.4",
        "typescript": "^5.3.3",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    }
}
