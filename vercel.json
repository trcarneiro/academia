{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "functions": {
    "api/serverless.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless"
    },
    {
      "source": "/health",
      "destination": "/api/serverless"
    },
    {
      "source": "/(.*)",
      "destination": "/api/serverless"
    }
  ]
}
