{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "functions": {
    "api/serverless.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/serverless"
    },
    {
      "src": "/health",
      "dest": "/api/serverless"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless"
    }
  ]
}
